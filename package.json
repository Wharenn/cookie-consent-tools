    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^2.1.1",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Wharenn/cookie-consent-tools.git"
